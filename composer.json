{
    "name": "mohamedsaad/ai-bridge",
    "description": "AI Bridge package to run Python similarity check inside Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Saad",
            "email": "mohamed.saadd@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/process": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Saad\\AiBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saad\\AiBridge\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saad\\AiBridge\\AiServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
